# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                               controllers.Application.home
GET     /assets/*file                   controllers.Assets.versioned(path="/public", file: Asset)
GET     /game                           controllers.Application.startGame
GET     /login/:player1/:player2        controllers.Application.login(player1: String, player2: String)
GET     /getMoves/:x/:y                 controllers.Application.getMoves(x: String, y: String)
GET     /moveFigure/:cpx/:cpy/:x/:y     controllers.Application.moveFigure(cpx: String, cpy: String, x: String, y: String)
GET     /socket                         controllers.Application.socket
GET     /chessPolymer                   controllers.Application.chessPolymer
